{
  "name": "excel-simplify-server",
  "version": "1.0.0",
  "description": "A Node Express RESTFUL API",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "prepare": "npx ts-patch install -s",
    "type-check": "tsc --noEmit",
    "build": "rm -rf dist && swc src -d dist",
    "start": "NODE_ENV=production node ./dist/index.js",
    "stop": "pm2 stop all",
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "NODE_ENV=development nodemon --exec node -r @swc-node/register ./src/index.ts",
    "stripe:dev": "stripe listen --forward-to localhost:3000/webhook",
    "lint": "eslint .",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "Excel Simplify",
  "license": "ISC",
  "dependencies": {
    "@axiomhq/axiom-node": "^0.12.0",
    "@sendgrid/mail": "^7.7.0",
    "cache-manager": "^5.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.3",
    "firebase-admin": "^11.5.0",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "joi": "^17.7.1",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "openai": "^3.2.1",
    "pm2": "^5.2.2",
    "stripe": "^11.11.0",
    "validator": "^13.9.0",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.37",
    "@swc/jest": "^0.2.24",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.5",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "prettier-plugin-sort-imports": "^1.7.1",
    "stripe-event-types": "^2.2.0",
    "swc": "^1.0.11",
    "swc-node": "^1.0.0",
    "ts-patch": "^2.1.0",
    "types-joi": "^2.1.0",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  }
}
